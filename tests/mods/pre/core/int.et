#!/usr/bin/env et

a = 5;
b = 10;

assert_eq( 1, 1 );
assert_ne( 0, 1 );
assert_lt( 1, 2 );
assert_le( 1, 1 );
assert_gt( 2, 1 );
assert_ge( 1, 1 );
assert( !0 );

assert_eq( 5 + 10, 15 );
assert_eq( 5 - 10, -5 );
assert_eq( 5 * 10, 50 );
assert_eq( 5 / 10,  0 );
assert_eq( 5 % 10,  5 );

assert_eq( a += 10, 15 );
assert_eq( a -= 10,  5 );
assert_eq( a *= 10, 50 );
assert_eq( a /= 10,  5 );
assert_eq( a %= 10,  5 );

assert_eq( 2 << 2, 8 );
assert_eq( 2 >> 1, 1 );

assert_eq( a <<= 2, 20 );
assert_eq( a >>= 2,  5 );

assert_eq( 5 & 4, 4 );
assert_eq( 5 | 3, 7 );
assert_eq( ~5,   -6 );

assert_eq( 5 ** 4, 625 );

assert_eq( -6, -6 );

assert_eq( int( '1252' ), 1252 );

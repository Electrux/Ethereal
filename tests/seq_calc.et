#!./buildfiles/et

import std.vec;
import std.str;

a = [ '5', '+', '9', '-', '2' ];

sz = a.len();

res = 0;
op = '';

for item in a.iter() {
	if item.is_int() {
		if op == '+' {
			res += item.to_int();
		} elif op == '-' {
			res -= item.to_int();
		} else {
			res = item.to_int();
		}
	} else {
		op = item;
	}
}

if args.len() < 2 {
	println( "final result: ", res );
} else {
	assert( res == 12, res, ' should be equal to: 5 + 9 - 2 = 12' );
}

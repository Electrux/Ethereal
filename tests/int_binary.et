#!./buildfiles/et

import vec;
import str;

num = 12;

if args.len() < 2 {
	num = scan( 'Enter number for integer to binary: ' ).to_int();
}

bin = '';
for ; num > 0; {
	bin = ( num % 2 ).to_str() + bin;
	num /= 2;
}

if args.len() < 2 {
	assert( bin == '1100' );
} else {
	println( 'Binary is: ', bin );
}
#!./buildfiles/et

import str;

mfn< int > add_one() {
	self += 1;
	return self;
}

mfn< str > append_( s ) {
	self += s;
	println( 'original length: ', self.len() );
	println( 'length + 1: ', self.len().add_one() );
	self += s;
	println( self );
}

"hi".append_( "1" );

mfn< str, int, flt > print2() {
	println( '2' );
}

1.print2();
'some'.print2();
( 1.2 ).print2();

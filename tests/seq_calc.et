#!./buildfiles/et

import vec;
import str;

a = [ '5', '+', '9', '-', '2' ];

sz = a.len();

res = 0;
op = '';

for x = 0; x < sz; x += 1 {
	if a[ x ].is_int() {
		if op == '+' {
			res += a[ x ].to_int();
		} elif op == '-' {
			res -= a[ x ].to_int();
		} else {
			res = a[ x ].to_int();
		}
	} else {
		op = a[ x ];
	}
}

if args.len() < 2 {
	println( "final result: ", res );
} else {
	assert( res == 12, res, ' should be equal to: 5 + 9 - 2 = 12' );
}
